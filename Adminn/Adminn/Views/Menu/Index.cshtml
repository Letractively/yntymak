@model IEnumerable<Adminn.Menu>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.food_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.food_content)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.food_price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.food_link)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.food_type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.food_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.food_content)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.food_price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.food_link)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.food_type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.food_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.food_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.food_id })
        </td>
    </tr>
}

</table>
